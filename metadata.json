{
  "name": "ppouliot-maas",
  "version": "0.2.0",
  "author": "Peter J. Pouliot <peter@pouliot.net>",
  "summary": "A puppet module to install and configure Ubuntu MAAS (Metal-As-A-Service)",
  "license": "Apache-2.0",
  "source": "https://github.com/ppouliot/puppet-maas",
  "project_page": "https://github.com/ppouliot/puppet-maas",
  "issues_url": "https://github.com/ppouliot/puppet-maas/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": " >= 3.0.0 < 5.0.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    }
  ]
}
